#compdef top 

_hello() { 
    local curcontext="$curcontext" state line
    typeset -A opt_args
    
    _arguments \
        '1: :->country'\
        '*: :->city'

    res=`python /Users/loganengstrom/projects/bash-predict/call_server.py $words $CURSOR`
    for i in $res;
    do
        compadd "$@" $i
    done
}

_hello "$@"
